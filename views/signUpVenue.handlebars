<div class=" d-flex justify-content-center">
      <div class="col-md-3">
    <h2 class="d-flex justify-content-center">Signup</h2>

  
    <form class="flex-column d-flex justify-content-center form signup-form">

      <div class="d-flex justify-content-center form-group">
        <label for="name-signup">Venue name:</label>
        <input class="form-input" type="text" id="name-signup" />
      </div>

      <div class="d-flex justify-content-center form-group">
        <label for="website-signup">website:</label>
        <input class="form-input" type="text" id="website-signup" />
      </div>

      <div class="d-flex justify-content-center form-group">
        <label for="email-signup">email:</label>
        <input class="form-input" type="text" id="email-signup" />
      </div>

      <div class="d-flex justify-content-center form-group">
        <label for="phone-signup">phone number:</label>
        <input class="form-input" type="text" id="phone-signup" />
      </div>

      <div class="d-flex justify-content-center form-group">
        <label for="location-signup">location:</label>
        <input class="form-input" type="text" id="location-signup" />
      </div>


