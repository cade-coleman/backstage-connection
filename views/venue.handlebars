{{#each venues as |venue| }}
<div class="venue-card card cjl" style="text-align:center;">
    <container class="row">
        <div class="col-md-4">
            <img src="..." class="card-img-top" alt="...">
        </div>
        <div class="card-body col-md-4">
            <header class="venue-card-header">{{venue.name}}</header>
            <ul>
                <li>{{venue.location}}</li>
                <li>{{venue.website}}</li>
                <li>{{venue.phone}}</li>
                <li>{{User.Venue.email}}</li>
            </ul>
        </div>
        <div class="col-md-4">{{venue.bio}}</div>
    </container>
</div>
{{/each}}

